.error('获取推荐失败', error);\n      }\n    },\n\n    async handleSearch() {\n      console.log('搜索', this.keyword);\n      try {\n        const response = await axios.get(`http://localhost:8080/api/papers/search?keyword=${this.keyword}&username=${this.username}`);\n        this.searchResult = response.data.data;\n        console.log(response);\n      } catch (error) {\n        console.error('搜索失败', error);\n      }\n    },\n\n    hideSearchPanel() {\n      setTimeout(() => {\n        this.showSearchPanel = false;\n      }, 200);\n    },\n\n    search() {\n      console.log('搜索', this.searchValue);\n    },\n    selectOption(index) {\n      this.selectedIndex = index;\n    },\n\n    handleAvatarError() {\n      this.avatarLoadError = true\n      this.userAvatar = null // 清空错误头像\n    },\n    showProfile() {\n      console.log('打开个人资料');\n    },\n    showNotifications() {\n      console.log('查看通知');\n    },\n    showSettings() {\n      console.log('打开设置');\n    },\n    logout() {\n      localStorage.removeItem('isLoggedIn');\n      this.$router.push('/');\n    }\n  },\n  mounted() {\n    console.log(this.$route.params);\n    this.username = this.$route.params.username || '321';\n    this.nickname = this.$route.params.nickname || 'Rick';\n    this.userAvatar = this.$route.params.avatarUrl || null;\n    this.getRecommend();\n  },\n  created() {\n    this.debouncedSearch = debounce(this.handleSearch, 1000);\n\n  }\n\n}\n</script>\n\n<style scoped src=\"../components/css/Home.css\"></style>"],"mappings":";AA+JA,OAAAA,KAAA;AACA,SAAAC,QAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,QAAA;MACAC,QAAA;MACAC,UAAA;MACAC,eAAA;MACAC,WAAA;MACAC,aAAA;MACAC,OAAA,GACA;QAAAC,IAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,IAAA;MAAA,EACA;MACAC,SAAA;MACAC,aAAA;MACAC,YAAA;MACAC,QAAA;MACAC,WAAA;MAAA;MACAC,eAAA;IACA;EACA;EACAC,QAAA;IACAC,SAAA;MACA,MAAAC,IAAA,OAAAC,IAAA,GAAAC,QAAA;MACA,IAAAF,IAAA;QACA;MACA,WAAAA,IAAA;QACA;MACA,WAAAA,I