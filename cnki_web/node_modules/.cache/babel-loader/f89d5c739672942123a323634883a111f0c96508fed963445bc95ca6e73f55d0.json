rLoadError = true;\n      this.userAvatar = null; // 清空错误头像\n    },\n    showProfile() {\n      console.log('打开个人资料');\n    },\n    showNotifications() {\n      console.log('查看通知');\n    },\n    showSettings() {\n      console.log('打开设置');\n    },\n    logout() {\n      localStorage.removeItem('isLoggedIn');\n      this.$router.push('/');\n    }\n  },\n  mounted() {\n    console.log(this.$route.params);\n    this.username = this.$route.params.username || '321';\n    this.nickname = this.$route.params.nickname || 'Rick';\n    this.userAvatar = this.$route.params.avatarUrl || null;\n    this.getRecommend();\n  },\n  created() {\n    this.debouncedSearch = debounce(this.handleSearch, 1000);\n  }\n};","map":{"version":3,"names":["axios","debounce","name","data","keyword","username","nickname","userAvatar","avatarLoadError","searchValue","selectedIndex","options","text","icon","recommend","searchHistory","searchResult","showMenu","unreadCount","debouncedSearch","computed","greeting","hour","Date","getHours","avatarInitial","charAt","toUpperCase","methods","viewPdf","pdfUrl","paperId","console","log","window","open","post","then","response","catch","error","getRecommend","get","handleSearch","hideSearchPanel","setTimeout","showSearchPanel","search","selectOption","index","handleAvatarError","showProfile","showNotifications","showSettings","logout","localStorage","removeItem","$router","push","mounted","$route","params","avatarUrl","created"],"sources":["src/views/Home.vue"],"sourcesContent":["<template>\n  <div class=\"background\">\n    <div class=\"navbar\">\n\n      <div class=\"option-tools\">\n        <div v-for=\"(option, index) in options\" :key=\"index\"\n          :class=\"['option-icon', { selected: selectedIndex === index }]\" @click=\"selectOption(index)\">\n          <div v-html=\"option.icon\"></div>\n          <div class=\"recommend-text\">{{ option.text }}</div>\n        </div>\n        <form class=\"search-bar\" @submit.prevent=\"search\">\n          <svg