{"ast":null,"code":"import UserPanel from '@/components/UserPanel/UserPanel.vue';\nimport NavCard from '@/components/NavCard/NavCard.vue';\nimport PaperCard from '@/components/PaperCard/PaperCard.vue';\nimport ActivityList from '@/components/ActivityList/ActivityList.vue';\nexport default {\n  name: 'HomePage',\n  components: {\n    UserPanel,\n    NavCard,\n    PaperCard,\n    ActivityList\n  },\n  data() {\n    return {\n      searchQuery: '',\n      currentUser: {\n        name: '王研究员',\n        avatar: require('@/assets/default-avatar.png'),\n        role: '高级会员'\n      },\n      navItems: [{\n        title: '我的论文',\n        icon: 'document-text',\n        count: 12\n      }, {\n        title: '文献收藏',\n        icon: 'bookmark',\n        count: 45\n      }, {\n        title: '下载记录',\n        icon: 'download',\n        count: 89\n      }, {\n        title: '学术圈子',\n        icon: 'user-group',\n        count: 3\n      }],\n      recommendedPapers: [{\n        id: 1,\n        title: '人工智能在材料科学中的应用进展',\n        authors: '张晓华, 李国强',\n        journal: '科学进展',\n        citations: 128,\n        year: 2023\n      }\n      // 更多文献数据...\n      ],\n      dailyStats: {\n        visits: 156,\n        collections: 23,\n        downloads: 45\n      },\n      recentActivities: [{\n        type: 'download',\n        title: '下载了《量子计算综述》',\n        time: '2小时前'\n      }\n      // 更多动态数据...\n      ]\n    };\n  },\n  methods: {\n    performSearch() {\n      // 搜索逻辑\n    },\n    handleNavClick(item) {\n      console.log('导航项点击:', item);\n      // 导航点击处理\n    },\n    viewPaper(id) {\n      console.log('查看文献:', id);\n      // 查看文献详情\n    }\n  }\n};","map":{"version":3,"names":["UserPanel","NavCard","PaperCard","ActivityList","name","components","data","searchQuery","currentUser","avatar","require","role","navItems","title","icon","count","recommendedPapers","id","authors","journal","citations","year","dailyStats","visits","collections","downloads","recentActivities","type","time","method